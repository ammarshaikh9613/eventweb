<!-- page title -->
<section class="page-title-section overlay" data-background="">
  <div class="container">
    <div class="row">
      <div class="col-md-8">
        <ul class="list-inline custom-breadcrumb">
          <li class="list-inline-item"><a class="h2 text-primary font-secondary" href="events.html">Booked Events</a></li>
       
        </ul>
      </div>
    </div>
  </div>
</section>
<!-- /page title -->
<% const m=getMessages() %>    
<% if (m.notify) { %>
<% m.notify.forEach((element) => { %>
 <div class="alert alert-success toastrDefaultSuccess" role="alert">
    <%= element %>
</div>
<% }) %>                           
<% } %>

  
<!-- courses -->
<section class="section">
    <div class="container d-flex justify-content-center mt-50 mb-50">
            
        <div class="row">
           <div class="col-md-12">
            <% if(db_rows_array.length>0){%>
                <% for(var i=0; i<db_rows_array.length;i++) {%>
        
                <div class="card card-body card border-0 rounded-0 hover-shadow">
                            <div class="media align-items-center align-items-lg-start text-center text-lg-left flex-column flex-lg-row">
                                <div class="mr-2 mb-3 mb-lg-0">
                                    
                                        <img src="<%=db_rows_array[i].event_photo%>" width="250" height="250" alt="">
                                   
                                </div>

                                <div class="media-body">
                                    
                                        <h4 class="card-title"><%=db_rows_array[i].event_name%></h4>
                                    

                                    <ul class="list-inline list-inline-dotted mb-3 mb-lg-2">
                                        <li class="list-inline-item"><a href="#" class="text-muted" data-abc="true"><i class="ti-calendar text-primary icon"></i> <%=db_rows_array[i].event_from_date%> - <%=db_rows_array[i].event_to_date%> </a></li>
                                        <li class="list-inline-item"><a href="#" class="text-muted" data-abc="true"> <i class="ti-time text-primary icon"></i> <%=db_rows_array[i].event_from_time%> - <%=db_rows_array[i].event_to_time%> </a></li>
                                    </ul>

                                    <p class="mb-3"><%=db_rows_array[i].event_details%> </p>

                                    <ul class="list-inline list-inline-dotted mb-0">
                                        <li class="list-inline-item"> <a href="#" data-abc="true"><i class="ti-location-pin text-primary mr-2"></i><%=db_rows_array[i].event_venue%></a></li>
                                       
                                    </ul>
                                </div>

                                <div class="mt-3 mt-lg-0 ml-lg-3 text-center">

                                 

                                    
                                    <a href="/viewprize/<%= db_rows_array[i].event_id%>" class="btn btn-success mt-4 text-white">View Prize</a>
           
                                </div>
                            </div>
                </div>
                <br>
                <% } %>
                <%}else{%>
                  No Record Found
                  <%}%>               
         </div>                     
        </div>
    </div>
</section>
<!-- /courses -->

